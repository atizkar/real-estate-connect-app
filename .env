# .env file for Docker Compose (at project root)

# Docker Hub Username (replace with your actual Docker Hub username)
DOCKER_USERNAME=knowlegepot

# Image Tags for Frontend and Backend
# Use the tags you pushed from your CI/CD (e.g., 'feature-laravel-backend' or 'latest' if on dev branch)
WEB_IMAGE_TAG=feature-laravel-backend
LARAVEL_IMAGE_TAG=feature-laravel-backend

# MySQL Database Credentials
# For local development, these can be simple, but use strong ones for production!
MYSQL_DATABASE=real_estate_db
MYSQL_USER=laravel_user
MYSQL_PASSWORD=dev_password
MYSQL_ROOT_PASSWORD=root_pass